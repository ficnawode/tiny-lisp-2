{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "test_closures",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/lisp/test_closures/test_closures_exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/test/lisp/test_closures/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
